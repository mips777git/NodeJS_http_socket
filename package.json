{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon app.js",
    "kill:8080": "lsof -ti:8080 | xargs kill -9",
    "kill:8081": "lsof -ti:8081 | xargs kill -9",
    "kill:all": "lsof -ti:8080 | xargs kill -9; lsof -ti:8081 | xargs kill -9",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:8080": "pm2 start ecosystem.config.js --only NodeSocket-8080",
    "pm2:start:8081": "pm2 start ecosystem.config.js --only NodeSocket-8081",
    "pm2:stop": "pm2 stop all",
    "pm2:stop:8080": "pm2 stop NodeSocket-8080",
    "pm2:stop:8081": "pm2 stop NodeSocket-8081",
    "pm2:restart": "pm2 restart all",
    "pm2:restart:8080": "pm2 restart NodeSocket-8080",
    "pm2:restart:8081": "pm2 restart NodeSocket-8081",
    "pm2:delete": "pm2 delete all",
    "pm2:delete:8080": "pm2 delete NodeSocket-8080",
    "pm2:delete:8081": "pm2 delete NodeSocket-8081",
    "pm2:logs": "pm2 logs",
    "pm2:logs:8080": "pm2 logs NodeSocket-8080",
    "pm2:logs:8081": "pm2 logs NodeSocket-8081",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:dev": "pm2 start ecosystem.config.js --watch",
    "pm2:prod": "pm2 start ecosystem.config.js --env production"
  },
